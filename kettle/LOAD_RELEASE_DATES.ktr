  <step>
    <name>Extract Release Dates</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>Y</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var status= 0;
var have_match= &apos;match&apos;;

var release_string= line_text.getString();
var movie_id = &apos;&apos;;
var country_name = &apos;&apos;; 
var day_number = &apos;&apos;;
var month_number = &apos;&apos;;
var year = &apos;&apos;;
var region_name = &apos;&apos;;

if(
  release_string.match(
    &#47;([^\t]+)\t+(.+):((([0-9]{1,2}) +)?([A-Za-z]+),? *)?([0-9]{4})(\t+\((.+)\))?&#47;i
&#47;&#47;   1          2    345               6                7         8     9
  )
){
  status= 1;
  movie_id = RegExp.$1; &#47;&#47;movie_id
  country_name = RegExp.$2; &#47;&#47;country_name
  day_number = (RegExp.$5==&apos;&apos;?0:RegExp.$5);

  switch(RegExp.$6){    
    case &apos;&apos;:
      month_number= 0;
      break;
    case &apos;January&apos;:
      month_number= 1;
      break;
    case &apos;February&apos;:
      month_number= 2;
      break;
    case &apos;March&apos;:
      month_number= 3;
      break;
    case &apos;April&apos;:
      month_number= 4;
      break;
    case &apos;May&apos;:
      month_number= 5;
      break;
    case &apos;June&apos;:
      month_number= 6;
      break;
    case &apos;July&apos;:
      month_number= 7;
      break;
    case &apos;August&apos;:
      month_number= 8;
      break;
    case &apos;September&apos;:
      month_number= 9;
      break;
    case &apos;October&apos;:
      month_number= 10;
      break;
    case &apos;November&apos;:
      month_number= 11;
      break;
    case &apos;December&apos;:
      month_number= 12;
      break;
    default:
      month_number= RegExp.$6;
  }
  year = RegExp.$7; &#47;&#47;year
  region_name = RegExp.$9; &#47;&#47;region_name
} else {
  &#47;&#47;TODO:discard this row
  have_match= &apos;no match&apos;;
}
</jsScript_script>
