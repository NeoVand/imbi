    <sql>drop table if exists ods_release;

CREATE TABLE ods_release (
  line_text    varchar(255)      NOT NULL DEFAULT &apos;&apos;,
  line_number  int unsigned      NOT NULL DEFAULT 0,
  movie_id     varchar(255)      NOT NULL DEFAULT 0,
  country_name varchar(64)       NOT NULL DEFAULT &apos;&apos;,
  day_number   tinyint unsigned  NOT NULL DEFAULT 0,
  month_number tinyint unsigned  NOT NULL DEFAULT 0,
  year         smallint unsigned NOT NULL DEFAULT 0,
  region_name  varchar(128)      NOT NULL DEFAULT &apos;&apos;,
  primary key(movie_id,country_name,region_name,year,month_number,day_number)
)
ENGINE=MyISAM 
DEFAULT CHARSET=latin1
DEFAULT COLLATE=latin1_general_cs
;</sql>
